<Project xmlns="http://schemas.microsoft.com/developer/msbuild/2003">
  <Target Name="CopyTensorFlowLiteSelectTfOps" BeforeTargets="CopyFilesToOutputDirectory">
    <ItemGroup>
      <TensorFlowLiteSelectTfOps Include="$(MSBuildThisFileDirectory)../contentFiles/TensorFlowLiteSelectTfOps.framework/**/*.*" />
    </ItemGroup>
    <Copy SourceFiles="@(TensorFlowLiteSelectTfOps)" DestinationFolder="$(DeviceSpecificIntermediateOutputPath)TensorFlowLiteSelectTfOps.framework/%(RecursiveDir)" ContinueOnError="false" />
    <ItemGroup>
      <NativeReference Include="$(DeviceSpecificIntermediateOutputPath)TensorFlowLiteSelectTfOps.framework">
        <Kind>Framework</Kind>
        <SmartLink>False</SmartLink>
        <ForceLoad>True</ForceLoad>
        <IsCxx>True</IsCxx>
        <LinkerFlags>-force_load $(DeviceSpecificIntermediateOutputPath)TensorFlowLiteSelectTfOps.framework/TensorFlowLiteSelectTfOps</LinkerFlags>
        <WeakFrameworks>CoreML</WeakFrameworks>
      </NativeReference>
    </ItemGroup>
  </Target>
</Project>
